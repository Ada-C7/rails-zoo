<%= form_for @animal, method: form_method do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :species %>
  <%= f.text_field :species %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :image_link %>
  <%= f.text_field :image_link %>
  <%= f.submit %>
<% end %>
