<%= form_for @animal do |f| %>
<ul>
  <li>
    <%= f.label :image_url, "Photo Link" %>
    <%= f.text_field :image_url %>
  </li>
  <li>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </li>

  <li>
    <%= f.label :species %>
    <%= f.text_field :species %>
  </li>

  <li>
    <%= f.label :age %>
    <%= f.number_field :age %>
  </li>

  <li>
    <%= f.submit nil %>
  </li>
</ul>
<% end %>
