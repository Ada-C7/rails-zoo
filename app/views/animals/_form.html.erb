<%= form_for @animal do |f| %>
  <%= f.label :name %></br>
  <%= f.text_field :name %></br>

  <%= f.label :species %></br>
  <%= f.text_field :species %></br>

  <%= f.label :age %></br>
  <%= f.text_field :age %></br>

  <%= f.label :age_unit %></br>
  <%= f.text_field :age_unit %></br>

  <%= f.label :image_link %></br>
  <%= f.text_field :image_link %></br>
  <%= f.submit %></br>
<% end %>
